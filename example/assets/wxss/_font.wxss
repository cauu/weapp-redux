@font-face {
  font-family: "iconfont";
  src: url('../fonts/iconfont.eot');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size: 16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.text-xxs {
  font: 10rpx Microsoft JhengHei, Hiragino Sans GB, NotoSans-Regular;
  font-size: 10rpx;
  font-family: Microsoft JhengHei, Hiragino Sans GB, NotoSans-Regular;
}

